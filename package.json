    "@typescript-eslint/parser": "4.5.0",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.13.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-promise": "4.2.1",
    "husky": "4.3.0",
    "jest": "26.6.0",
